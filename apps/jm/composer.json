{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1",
        "enqueue/amqp-ext": "^0.4.0@dev",
        "enqueue/enqueue-bundle": "^0.4.11@dev",
        "formapro/php-quartz": "dev-master",
        "formapro/pvm": "dev-master",
        "makasim/values": "@dev",
        "makasim/yadm": "@dev",
        "makasim/yadm-bundle": "@dev",
        "symfony/console": "^3.3",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^3.3",
        "symfony/yaml": "^3.3",
        "justinrainbow/json-schema": "^5",
        "ramsey/uuid": "^3.6"
    },
    "require-dev": {
        "symfony/dotenv": "^3.3"
    },
    "config": {
        "platform": {
            "php": "7.1"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "make cache-warmup": "script",
            "assets:install --symlink --relative %WEB_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "<3.3",
        "symfony/twig-bundle": "<3.3",
        "symfony/debug": "<3.3"
    },
    "extra": {
        "symfony": {
            "id": "01BH20HT59R1KN3CEXRM17KTRV",
            "allow-contrib": "true"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:formapro/pvm.git"
        },
        {
            "type": "git",
            "url": "git@github.com:formapro/php-quartz.git"
        }
    ]
}
